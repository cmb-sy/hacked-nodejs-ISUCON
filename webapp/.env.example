# ISHOCON1 Database Configuration
# Copy this file to .env and modify the values as needed

# Database host (default: localhost)
ISHOCON1_DB_HOST=localhost

# Database port (default: 3306)
ISHOCON1_DB_PORT=3306

# Database user (default: root)
ISHOCON1_DB_USER=root

# Database password (default: root)
# For local development, you may need to set this to match your MySQL root password
# Leave empty if your MySQL root user has no password
ISHOCON1_DB_PASSWORD=root

# Database name (default: ishocon1)
ISHOCON1_DB_NAME=ishocon1
